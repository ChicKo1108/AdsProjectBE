{
  "name": "expressframe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "cross-env NODE_ENV=development node ./bin/www",
    "start:prod": "cross-env NODE_ENV=production node ./bin/www",
    "dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "check-env": "node scripts/load-env.js",
    "check-env:dev": "cross-env NODE_ENV=development node scripts/load-env.js",
    "check-env:prod": "cross-env NODE_ENV=production node scripts/load-env.js",
    "migrate": "knex migrate:latest",
    "migrate:dev": "cross-env NODE_ENV=development knex migrate:latest",
    "migrate:prod": "cross-env NODE_ENV=production knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "seed:dev": "cross-env NODE_ENV=development knex seed:run",
    "seed:prod": "cross-env NODE_ENV=production knex seed:run",
    "seed:make": "knex seed:make",
    "test": "cross-env NODE_ENV=test jest",
    "test:auth": "cross-env NODE_ENV=test jest test/auth.test.js",
    "test:ad-creatives": "cross-env NODE_ENV=test jest test/adCreatives.test.js",
    "test:ad-plan": "cross-env NODE_ENV=test jest test/adPlan.test.js",
    "test:ad-group": "cross-env NODE_ENV=test jest test/adGroup.test.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.2",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^0.21.15",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  }
}
